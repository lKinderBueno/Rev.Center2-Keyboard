                    <Path x:Name="grip"
                        Data="M 12,11 C 12,11 18,5.5 18,5.5 18,5.5 12,0 12,0 12,0 0,0 0,0 0,0 0,11 0,11 0,11 12,11 12,11 Z"
                        Fill="{StaticResource SliderThumb.Static.Background}" Stretch="Fill"
                        Stroke="{StaticResource SliderThumb.Static.Border}"/>
                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="IsMouseOver" Value="true">
                        <Setter TargetName="grip" Value="{StaticResource SliderThumb.MouseOver.Background}" Property="Fill"/>
                        <Setter TargetName="grip" Value="{StaticResource SliderThumb.MouseOver.Border}" Property="Stroke"/>
